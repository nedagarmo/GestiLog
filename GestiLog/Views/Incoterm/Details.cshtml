@model GestiLog.Entities.Incoterm

@{
    ViewBag.Title = "Incoterm";
}

<div class="row">
    <!-- column -->
    <div class="col-md-12">
        <!-- general form elements -->
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Información Ampliada</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <dl class="dl-horizontal">
                    <dt>
                        Descripción
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Descripcion)
                    </dd>

                    <dt>
                        Código
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Codigo)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Habilitado)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Habilitado)
                    </dd>

                </dl>
            </div>
            <!-- /.box-body -->
            <div class="box-footer text-right">
                @Html.ActionLink("Volver", "Index", null, new { @class = "btn btn-danger btn-sm" })
                @Html.ActionLink("Editar", "Edit", new { id = Model.Id }, new { @class = "btn btn-default btn-sm" })
            </div>
        </div>
        <!-- /.box -->
    </div>
    <!--/.col -->
</div>
<!-- /.row -->